{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    // 1) Try to serve a static file from /public if it exists
    { "handle": "filesystem" },

    // 2) Otherwise, fall back to your Express server
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
